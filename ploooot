set xr [0:21]
set yr [0:1]
set xlabel 'x'
set ylabel 'z'
plot 'end-2' u 1:3 w l t 'topography', \
     'end-2' u 1:2 w l t '0.02-2 layers', \
     'end-5' u 1:2 w l t '0.02-5 layers', \
     'log'   u 1:2 w l t '0.02-15 layers',\
     'end-20' u 1:2 w l t '0.02-20 layers', \
     'end-30' u 1:2 w l t '0.02-30 layers'

